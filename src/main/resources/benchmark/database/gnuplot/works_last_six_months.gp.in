set title "XWHEP : Jobs last six months, per week\nTITLE"
set xlabel "Weeks"
set ylabel "Jobs"
set encoding iso_8859_1
set key outside
#set key 80, 535
#set key 1,3
#set size 1.3,1
set key spacing 1.5
set key width 1.5
#set logscale y
set xtics rotate



set terminal png
set output 'FILEOUT_works_last_six_months.png'

plot 'FILEIN_completeds_last_six_months.csv' using 1:2 with lines  lt 2  lw 2 title 'Completeds'
plot 'FILEIN_errors_last_six_months.csv'     using 1:2 with lines  lt 2  lw 2 title 'Errors'
plot 'FILEIN_started_last_six_months.csv'   using 1:2 with lines  lt 2  lw 2 title 'Started'
plot 'FILEIN_restarted_last_six_months.csv' using 1:2 with lines  lt 2  lw 2 title 'Restarted'
plot 'FILEIN_submitted_last_six_months.csv'  using 1:2 with lines  lt 2  lw 2 title 'Submitted'
